{
  "version": 2,
  "builds": [
    { "src": "routes/**/*.js", "use": "@vercel/node" },
    { "src": "static/**", "use": "@vercel/static" },
    { "src": "script/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "/app.js"
    }
  ],
  "cleanUrls": true
}
